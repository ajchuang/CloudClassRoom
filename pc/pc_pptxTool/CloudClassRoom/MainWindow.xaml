<Window x:Class="WpfApplication1.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Cloud Class Room" Topmost="True" mc:Ignorable="d" xmlns:d="http://schemas.microsoft.com/expression/blend/2008" xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" Height="147" Width="209" ResizeMode="NoResize">
    <Grid Height="112" Width="183">
        <Button Height="38" HorizontalAlignment="Left" Margin="12,66,0,0" Name="button1" VerticalAlignment="Top" Width="70" Click="prevBtn_Click">
            <Image Source="Images\previous.png"></Image>
        </Button>
        <Button Height="37" HorizontalAlignment="Left" Margin="102,66,0,0" Name="button2" VerticalAlignment="Top" Width="70" Click="nextBtn_Click">
            <Image Source="Images\next.png"></Image>
        </Button>
        <Button Height="38" HorizontalAlignment="Left" Margin="12,12,0,0" Name="button3" VerticalAlignment="Top" Width="70" Click="openBtn_Click" DataContext="{Binding}">
            <Image Source="Images\open.png"> </Image>
        </Button>
        <Button Height="38" HorizontalAlignment="Left" Margin="102,12,0,0" Name="button4" VerticalAlignment="Top" Width="70" Click="closeBtn_click">
            <Image Source="Images\exit.png"></Image>
        </Button>
    </Grid>
</Window>
